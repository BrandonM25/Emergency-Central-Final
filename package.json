    "mysql2": "^1.5.3",
    "path": "^0.12.7",
    "react-scripts": "^1.1.4",
    "sequelize": "^4.37.6",
    "twilio": "^3.16.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
